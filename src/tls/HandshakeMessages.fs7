(* resumption log predicates *)

predicate ServerLogBeforeServerFinishedResume of abbrInfo * SessionInfo * log
definition !ai,si,l.
  ServerLogBeforeServerFinishedResume(ai,si,l) <=>
  (?cpv,csl,cml,ex1,ex2,ctl,sal,nl,csid.
    B(l) = (ClientHelloMsg(cpv,ai.abbr_crand,csid,csl,cml,ex1)  @|
           (ServerHelloMsg(si.protocol_version,ai.abbr_srand,si.sessionID,si.cipher_suite,si.compression,ex2))))

predicate ServerLogBeforeClientFinishedResume of abbrInfo * SessionInfo * log
definition !ai,si,l.
  ServerLogBeforeClientFinishedResume(ai,si,l) <=>
  (?l',svd.   ServerLogBeforeServerFinishedResume(ai,si,l') /\
    B(l) = B(l') @| ServerFinishedMsg(svd))

predicate ClientLogBeforeServerFinishedResume of abbrInfo * SessionInfo * log
definition !ai,si,l.
  ClientLogBeforeServerFinishedResume(ai,si,l) <=>
  (?l',ex2. ClientLogBeforeServerHello(ai.abbr_crand,l')  /\
    B(l) = B(l') @|
           ServerHelloMsg(si.protocol_version,ai.abbr_srand,si.sessionID,si.cipher_suite,si.compression,ex2))

predicate ClientLogBeforeClientFinishedResume of abbrInfo * SessionInfo * log
definition !ai,si,l.
  ClientLogBeforeClientFinishedResume(ai,si,l) <=>
  (?l',svd.   ClientLogBeforeServerFinishedResume(ai,si,l') /\
    B(l) = B(l') @| ServerFinishedMsg(svd))

(* -------------------------------------------------------------------- *)
ask !si,si',t. ClientLogBeforeCertificateVerifyRSA_Auth(si,t) /\
                   ServerLogBeforeClientCertificateVerifyRSA(si',t) =>
                   UpdatesClientSigAlg(si,si')

ask !si,si',t. ClientLogBeforeCertificateVerifyDHE_Auth(si,t) /\
                   ServerLogBeforeClientCertificateVerifyDHE(si',t) =>
                   UpdatesClientSigAlg(si,si')

ask !si,si',t. ClientLogBeforeClientFinished(si,t) /\
                   ServerLogBeforeClientFinished(si',t) =>
		   si = si'

ask !si,si',t. ClientLogBeforeServerFinished(si,t) /\
                   ServerLogBeforeServerFinished(si',t) =>
		   si = si'

ask !ai,si,ai',si',t.
                   ClientLogBeforeClientFinishedResume(ai,si,t) /\
                   ServerLogBeforeClientFinishedResume(ai',si',t) =>
	  	   (ai.abbr_crand = ai'.abbr_crand /\
                    ai.abbr_srand = ai'.abbr_srand /\
                    si.sessionID = si'.sessionID)

ask !ai,si,ai',si',t.
                   ClientLogBeforeServerFinishedResume(ai,si,t) /\
                   ServerLogBeforeServerFinishedResume(ai',si',t) =>
	  	   (ai.abbr_crand = ai'.abbr_crand /\
                    ai.abbr_srand = ai'.abbr_srand /\
                    si.sessionID = si'.sessionID)

ask !cr,sr,p,g,gs,cr',sr',p',g',gs',t.
           B(t) = B(cr) @| B(sr) @| DHEParamBytes(p,g,gs) /\
           Length(cr) = 32 /\
           Length(sr) = 32 /\
           B(t) = B(cr') @| B(sr') @| DHEParamBytes(p',g',gs') /\
           Length(cr') = 32 /\
           Length(sr') = 32 =>
           (cr = cr' /\ sr = sr' /\ p = p' /\ g = g' /\ gs = gs')

(* disjointness rules *)
ask !si,si',t. ClientLogBeforeCertificateVerifyRSA_Auth(si,t) /\
                   ServerLogBeforeClientCertificateVerifyDHE(si',t) => false

ask !si,si',t. ClientLogBeforeCertificateVerifyDHE_Auth(si,t) /\
                   ServerLogBeforeClientCertificateVerifyRSA(si',t) => false

ask !si,t,cr,sr,p,g,gs. ServerLogBeforeClientCertificateVerifyRSA(si,t) /\
	           Length(cr) = 32 /\ Length(sr) = 32 /\
	           B(t) = B(cr) @| B(sr) @| DHEParamBytes(p,g,gs)  => false

ask !si,t,cr,sr,p,g,gs. ServerLogBeforeClientCertificateVerifyDHE(si,t) /\
	           Length(cr) = 32 /\ Length(sr) = 32 /\
	           B(t) = B(cr) @| B(sr) @| DHEParamBytes(p,g,gs)  => false

ask !si,t,cr,sr,p,g,gs. ClientLogBeforeCertificateVerifyDHE_Auth(si,t) /\
	           Length(cr) = 32 /\ Length(sr) = 32 /\
	           B(t) = B(cr) @| B(sr) @| DHEParamBytes(p,g,gs)  => false

ask !si,t,cr,sr,p,g,gs. ClientLogBeforeCertificateVerifyRSA_Auth(si,t) /\
	           Length(cr) = 32 /\ Length(sr) = 32 /\
	           B(t) = B(cr) @| B(sr) @| DHEParamBytes(p,g,gs)  => false

ask !si,ai,si',t. ClientLogBeforeClientFinished(si,t) /\
                   ServerLogBeforeClientFinishedResume(ai,si',t)  => false

ask !si,ai,si',t. ClientLogBeforeServerFinished(si,t) /\
                   ServerLogBeforeServerFinishedResume(ai,si',t)  => false

ask !si,ai,si',t. ServerLogBeforeClientFinished(si,t) /\
                   ClientLogBeforeClientFinishedResume(ai,si',t)  => false

ask !si,ai,si',t. ServerLogBeforeServerFinished(si,t) /\
                   ClientLogBeforeServerFinishedResume(ai,si',t)  => false

(* -------------------------------------------------------------------- *)
predicate HasHTMessage of HandshakeType * cbytes

definition !ht. HasHTMessage(ht, [||]) <=> false
definition !ht,htd,data,l. HasHTMessage(ht, l @| MessageBytes(htd,data)) <=> (ht = htd \/ HasHTMessage(ht,l))

(* -------------------------------------------------------------------- *)
ask !htd,data. [||] @| MessageBytes(htd,data) = MessageBytes(htd,data)

ask !ht,htd,data.
      HasHTMessage(ht, MessageBytes(htd, data)) <=> (ht = htd)

ask !ht,htd1,htd2,data1,data2.
      HasHTMessage(ht, MessageBytes(htd1, data1) @| MessageBytes(htd2, data2))
  <=> (ht = htd1 \/ ht = htd2)

ask !ht,htd1,htd2,htd3,data1,data2,data3.
      HasHTMessage(ht,    MessageBytes(htd1, data1)
                       @| MessageBytes(htd2, data2)
                       @| MessageBytes(htd3, data3))
  <=> (ht = htd1 \/ ht = htd2 \/ ht = htd3)

(* -------------------------------------------------------------------- *)
ask !ai,si,t. ClientLogBeforeClientFinishedResume(ai,si,t) =>
                 HasHTMessage(HT_certificate,B(t)) => false
