    (si.client_auth = true /\ ClientLogBeforeCertificateRequestRSA(si',l') /\
     UpdatesClientAuth(si',si) /\
     B(l) = B(l') @| CertificateRequestMsg(pv,ctl,sal,nl)) =>
     ClientLogBeforeServerHelloDoneRSA(si,l)

